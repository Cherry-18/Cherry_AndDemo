<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.cherry.cherry_anddemo.ui.moduletest.ModuleTestActivity">

    <Button
        android:id="@+id/btn_moduletest"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ModuleTest"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="20dp"/>

    <Button
        android:id="@+id/btn_goto_shop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="gotoShop"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="20dp"/>

    <Button
        android:id="@+id/btn_goto_pay"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="gotoPay"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="20dp"/>
</LinearLayout>
