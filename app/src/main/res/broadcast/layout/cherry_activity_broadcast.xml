<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".ui.interview.broadcast.BroadcastActivity">

    <Button
        android:id="@+id/broadcast_btn0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="静态注册--普通广播"/>
    <Button
        android:id="@+id/broadcast_btn3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="静态注册--有序广播"/>

    <Button
        android:id="@+id/broadcast_btn1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="动态注册--普通广播"/>

    <Button
        android:id="@+id/broadcast_btn2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="动态注册--本地广播"/>

    <Button
        android:id="@+id/broadcast_btn4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="动态注册--有序广播"/>


</LinearLayout>